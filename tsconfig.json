{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "esModuleInterop": true,
    "lib": ["es2017", "DOM"],
    "module": "commonjs",
    "noEmitOnError": true,
    "noImplicitReturns": true,
    "outDir": "dist",
    "resolveJsonModule": true,
    "sourceMap": true,
    "target": "es2017"
  },
  "include": ["src"],
  "typeRoots": [
    "./src/services/types.ts",
  ]
}
